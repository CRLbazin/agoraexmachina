<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{{params.website.title}}{% endblock %}</title>
        {% block stylesheets %}
			<link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet"/>
			<link href="{{ asset('css/main.css') }}" rel="stylesheet"/>
			<script src="https://kit.fontawesome.com/5b7521421d.js" crossorigin="anonymous"></script>
		{% endblock %}
    </head>
    <body>
		{% include 'menu.html.twig' %}
		{% block body %}{% endblock %}
		{% block javascript %}
			<script src="{{ asset('js/jquery-3.4.1.slim.min.js') }}"></script>
			<script src="{{ asset('js/bootstrap.bundle.min.js') }}"></script>
			<script src="{{ asset('js/bootstrap-confirmation.min.js') }}"></script>
			<script src="{{ asset('js/ckeditor/ckeditor.js') }}" type="text/javascript"></script>
			<script src="{{ asset('js/ckeditor/adapters/jquery.js') }}" type="text/javascript"></script>
			<script src="{{ asset('js/main.js') }}"></script>
		{% endblock %}
	</body>	
</html>
